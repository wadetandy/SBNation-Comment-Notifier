{
  "name": "SBNation Comments Notifier",
  "version": "1.0",
  "description": "The first extension that I made.",
  "page_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["jquery.js", "code.js"]
    }
  ],
  "permissions": [
    "notifications"
  ],
  "run_at": "document_idle"
}
